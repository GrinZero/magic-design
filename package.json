{
	"name": "@source-bug/magic-design-react",
	"version": "0.0.7",
	"description": "A component library that can achieve magic effects",
	"author": "774933704@qq.com",
	"main": "./src/main.tsx",
	"types": "./dist/src/components/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/es/index.js",
			"require": "./dist/lib/index.js",
			"default": "./dist/magic-design-react.es.js"
		}
	},
	"engines": {
		"node": ">=16",
		"npm": ">=8",
		"pnpm": ">=6"
	},
	"keywords": [
		"ui",
		"frontend",
		"component",
		"components",
		"react component",
		"react components",
		"source",
		"sourcebug",
		"magic",
		"magic-design",
		"magicDesign",
		"weather",
		"swiper",
		"vite",
		"react",
		"typescript"
	],
	"packageManager": "pnpm@7.9.0",
	"scripts": {
		"publish": "npm run build:npm && npm publish --access public",
		"build:npm": "pnpm run reset:vite && tsc && vite build",
		"dev": "pnpm run reset:vite && cross-env DEBUG=vite:plugin:mkcert HTTPS=true vite",
		"dev:http": "pnpm run reset:vite && vite",
		"build": "pnpm run reset:vite && tsc && vite build",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"preview": "pnpm run build && vite preview",
		"analyze": "pnpm run reset:vite && cross-env ANALYZE=true pnpm run build",
		"mock:server": "json-server --watch ./mock/index.json --config ./mock/config.json",
		"reset:vite": "rimraf ./.vite-cache ./dist",
		"reset:file": "node ./script/clean.js",
		"reset": "pnpm run reset:vite && pnpm run reset:file",
		"prepare": "npx husky install"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GrinZero/magic-design.git"
	},
	"bugs": {
		"url": "https://github.com/GrinZero/magic-design/issues"
	},
	"homepage": "https://github.com/GrinZero/magic-design#readme",
